        "mysql2": "^3.14.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.5.7",
        "prisma": "^5.13.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "~5.8.3"
    }
